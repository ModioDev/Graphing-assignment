
public class Cordinates {
	public int x;
	public int yp;
	public int yn;
	
	public  Cordinates(double one, double ypos, double yneg)
	{
		x=(int) one;
		yp=(int) ypos;
		yn=(int) yneg;
	}
	
	public int getX()
	{
		return x;
	}
	
	public int getYNeg()
	{
		return yn;
	}
	
	public int getYPos()
	{
		return yp;
	}

}
